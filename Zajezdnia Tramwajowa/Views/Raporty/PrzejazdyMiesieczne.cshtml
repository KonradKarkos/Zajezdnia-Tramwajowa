@model Dictionary<Maszynista, int>

@{
    ViewBag.Title = "Przejazdy Miesieczne";
}

<h2>Liczba przejazdów przypisanych do danych motorniczych w tym miesiącu</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.First().Key.Stawka)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.First().Key.Imie)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.First().Key.Nazwisko)
        </th>
        <th>
            @Html.DisplayName("Przejazdy")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Key.Stawka)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Key.Imie)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Key.Nazwisko)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Value)
        </td>
    </tr>
}

</table>
<p>
    @Html.ActionLink("Powrót do listy raportów", "Index")
</p>
